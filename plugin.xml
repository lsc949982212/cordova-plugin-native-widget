<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-native-widget" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <name>nativeWidget</name>
    <js-module name="nativeWidget" src="www/nativeWidget.js">
        <clobbers target="nativeWidget"/>
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="nativeWidget">
                <param name="android-package" value="cordova.plugin.nativeWidget.nativeWidget"/>
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/nativeWidget.java" target-dir="src/cordova/plugin/nativeWidget/nativeWidget"/>
    </platform>
</plugin>